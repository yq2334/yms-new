.wallet {
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: cover;
}
.wallet-content {
  padding: 21rpx;
}
.wallet-content .pannel {
  background: #FFFFFF;
  box-shadow: 0rpx 3rpx 21rpx 0rpx rgba(64, 64, 64, 0.15);
  border-radius: 26rpx;
}
.wallet-content .pannel .top .title {
  width: 257rpx;
  height: 64rpx;
  padding-left: 62rpx;
  line-height: 64rpx;
  position: relative;
  font-size: 32rpx;
  color: #FFFFFF;
}
.wallet-content .pannel .top .title::before {
  content: '';
  position: absolute;
  width: 6rpx;
  height: 24rpx;
  background: #FA8E1C;
  border-radius: 3rpx;
  left: 40rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.wallet-content .pannel .top .title.bg-1 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.wallet-content .pannel .top .title.bg-2 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.wallet-content .pannel .top .title.bg-3 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.wallet-content .pannel .top .title.bg-4 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.wallet-content .pannel .balance {
  position: relative;
}
.wallet-content .pannel .balance ._span {
  font-size: 28rpx;
  color: #1D1D1D;
  opacity: 0.8;
}
.wallet-content .pannel .balance ._h1 {
  font-size: 42rpx;
  font-family: 'AlimamaFangYuanTiVF';
  color: #1D1D1D;
  padding-left: 19rpx;
}
.wallet-content .pannel .balance::after {
  position: absolute;
  content: '';
  width: 100%;
  height: 1rpx;
  background-color: rgba(0, 0, 0, 0.1);
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  bottom: 0;
}
.wallet-content .pannel .balance .u-button {
  background: linear-gradient(90deg, #FA8E1C, #FABF5A);
  border-radius: 47rpx !important;
  color: #FFFFFF !important;
  font-size: 36rpx !important;
  border: 0 !important;
  font-weight: bold;
  width: 125rpx !important;
  height: 58rpx !important;
  border-radius: 29rpx !important;
  margin: 0;
}
.wallet-content .pannel .datas ._h3 {
  font-size: 28rpx;
  color: #1D1D1D;
  opacity: 0.8;
  line-height: 1.5;
}
.wallet-content .pannel .datas ._h1 {
  font-size: 32rpx;
  font-family: 'AlimamaFangYuanTiVF';
  color: #000;
  line-height: 1.5;
}
.wallet-content .pannel .u-button--primary .u-button__text {
  font-size: 31rpx !important;
}
.wallet-content .pannel .u-button--text .u-button__text {
  font-size: 32rpx !important;
  color: rgba(0, 0, 0, 0.8);
}

