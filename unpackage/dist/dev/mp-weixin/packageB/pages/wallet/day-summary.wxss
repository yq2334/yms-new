.summary-content {
  padding: 27rpx;
}
.summary-content .summary-data {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  padding: 0 37rpx;
}
.summary-content .summary-data .top {
  padding: 38rpx 0 30rpx 0;
  border-bottom: 1rpx solid rgba(43, 43, 43, 0.2);
}
.summary-content .summary-data .top .title {
  font-size: 28rpx;
  color: #2B2B2B;
  position: relative;
  padding-left: 38rpx;
}
.summary-content .summary-data .top .title:before {
  content: '';
  position: absolute;
  width: 6rpx;
  height: 20rpx;
  background: linear-gradient(125deg, #FF9647, #FECC52);
  border-radius: 3rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  left: 16rpx;
}
.summary-content .summary-data .datas {
  line-height: 1;
  padding-top: 22rpx;
  padding-bottom: 38rpx;
}
.summary-content .summary-data .datas ._h3 {
  font-size: 30rpx;
  color: rgba(43, 43, 43, 0.6);
  margin-bottom: 26rpx;
}
.summary-content .summary-data .datas ._h1 {
  font-size: 38rpx;
  color: #2B2B2B;
  font-family: 'AlimamaFangYuanTiVF';
}
.summary-content .summary-list {
  margin-top: 25rpx;
}
.summary-content .summary-list .item {
  padding: 28rpx 30rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 15rpx 0rpx rgba(75, 75, 75, 0.35);
  border-radius: 16rpx;
  font-size: 32rpx;
  color: #656565;
  line-height: 1;
  margin-bottom: 18rpx;
}
.summary-content .summary-list .item .split {
  height: 1rpx;
  background: #6F6F6F;
  opacity: 0.2;
  margin: 28rpx 0;
}
.summary-content .summary-list .item .fs {
  color: #1c1c1c;
}

